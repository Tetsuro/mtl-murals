{
    "extends": "airbnb",
    "globals": {
      "window": true,
      "document": true,
      "fetch": true
    },
    "rules": {
      "react/jsx-filename-extension": [1, {
          "extensions": [".js", ".jsx"]
      }],
      "react/prop-types": 0,
      "react/destructuring-assignment": 0,
      "jsx-a11y/mouse-events-have-key-events": 0,
      "react/jsx-one-expression-per-line": 0
    }
}